{
  "name": "@stacksjs/analytics",
  "type": "module",
  "version": "0.1.0",
  "description": "A privacy-first analytics toolkit for web applications.",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/analytics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/analytics.git"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/analytics/issues"
  },
  "keywords": ["analytics", "privacy", "typescript", "bun", "dynamodb"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "analytics": "./bin/cli.ts"
  },
  "files": ["bin", "dist", "src"],
  "scripts": {
    "analytics": "bun ./bin/cli.ts",
    "build": "bun --bun build.ts",
    "fresh": "bunx rimraf node_modules/ bun.lock && bun i",
    "lint": "bunx --bun pickier lint",
    "lint:fix": "bunx --bun pickier lint --fix",
    "changelog": "bunx logsmith --verbose",
    "changelog:generate": "bunx logsmith --output CHANGELOG.md",
    "release": "bun run changelog:generate && bunx bumpx prompt --recursive",
    "postinstall": "bunx git-hooks",
    "prepublishOnly": "bun --bun run build",
    "test": "bun test",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "dependencies": {
    "dynamodb-tooling": "^0.3.2"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@stacksjs/bumpx": "^0.2.4",
    "@stacksjs/gitlint": "^0.1.5",
    "@stacksjs/logsmith": "^0.2.1",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/vue3": "^8.4.7",
    "@storybook/vue3-vite": "^8.4.7",
    "@types/bun": "^1.3.3",
    "better-dx": "^0.2.5",
    "buddy-bot": "^0.5.0",
    "bun-git-hooks": "^0.3.1",
    "bun-plugin-dtsx": "^0.21.17",
    "pickier": "^0.1.10",
    "storybook": "^8.4.7",
    "typescript": "^5.9.3",
    "unocss": "^66.1.0",
    "vue": "^3.5.13"
  }
}
